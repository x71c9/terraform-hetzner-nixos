# Copy this file to .env and fill in your values
# Source this file before running terraform: source .env

export TF_VAR_hetzner_cloud_token="your-hetzner-token-here"
export TF_VAR_ssh_key_path="~/.ssh/id_ed25519.pub"
# Optional: only set if the key is not loaded in ssh-agent
export TF_VAR_ssh_private_key_path="~/.ssh/id_ed25519"